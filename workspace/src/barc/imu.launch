<launch>
	<!-- DATA SERVICE-->
	<node 
		pkg="data_service"
		type="service.py"
		name="data_service"
	>
	</node>
	
	<!-- IMU NODE -->
	<node 
		pkg="barc"
		type="imu_data_acq.py"
		name="imu_node"
	>
		<param name="port" value="/dev/ttyACM0" />
	</node>


	<!-- STATE ESTIMATION NODE -->
	<node 
		pkg="barc"
		type="state_estimation.py"
		name="state_estimation"
	>
		<param name="L_a" value="0.14" />	
		<param name="L_b" value="0.14" />	
	</node>
	
</launch>
