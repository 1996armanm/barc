<launch>
    <machine name="odroid" address="192.168.100.100" user="odroid" timeout="20.0" env-loader="$HOME/barc/scripts/remote-machines/env_loader_odroid.sh"/>
    <machine name="pc" address="192.168.100.102" timeout="20.0" />

   	<!-- GPS -->
  	<node machine="odroid" pkg="marvelmind_nav" type="hedge_rcv_bin" name="hedge_pos" >
  		<param name="port" value="dev/ttyACM1" />
  	</node>

    <!-- ARDUINO -->
  <node machine="odroid" pkg="rosserial_python" type="serial_node.py" name="arduino_node" >
    <param name="port" value="/dev/ttyUSB0" />
  </node>

</launch>
